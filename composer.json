{
  "name": "joesama/stress-pest",
  "description": "Pest Stress Plugin",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "ahmed johari jumali",
      "email": "joharijumali@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Joesama\\StressPest\\": "plugins/"
    }
  },
  "require": {
    "php": "^8.2",
    "ext-zip": "*",
    "barryvdh/laravel-snappy": "^1.0",
    "illuminate/database": "v10|v11|v12",
    "illuminate/config": "v10|v11|v12",
    "illuminate/support": "v10|v11|v12",
    "pestphp/pest": "^2.36",
    "pestphp/pest-plugin-stressless": "^2.2"
  },
  "require-dev": {
    "pheromone/phpcs-security-audit": "^2.0",
    "phpstan/phpstan": "^2.1"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
